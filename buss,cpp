#include <iostream>
 
 #include <map>
  #include <cmath>
 #include <algorithm>
using namespace std;
//#define int long long int
 
void solve()
{ 

int n;
cin>>n;
  if (n < 4 || n % 2 != 0) {
        cout << -1 << endl;
    }
else{
		long long maxi=n/4;
			long long mini=ceil(n/6.0);
		cout<<mini<<" "<<maxi<<endl;
}


	
}
 
 
 
    
 
int32_t main() {
    int t;
    cin >>t;
    
   
    
    while (t--) {
    	solve();
        
      
       
    }
    
    return 0;
}